<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>InvestorServlet</servlet-name>
  	<display-name>InvestorServlet</display-name>
  	<description></description>
  	<servlet-class>com.tcs.inv.InvestorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransactionServlet</servlet-name>
  	<display-name>TransactionServlet</display-name>
  	<description></description>
  	<servlet-class>com.tcs.inv.TransactionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CookieServlet</servlet-name>
  	<display-name>CookieServlet</display-name>
  	<description></description>
  	<servlet-class>com.tcs.inv.CookieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InvestorServlet</servlet-name>
  	<url-pattern>/inv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransactionServlet</servlet-name>
  	<url-pattern>/trans</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CookieServlet</servlet-name>
  	<url-pattern>/cookie</url-pattern>
  </servlet-mapping>
</web-app>
